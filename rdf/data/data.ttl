#Region
ex:islay     a    place:Region ;
        dcterms:name  "Islay"@en .
ex:highland  a    place:Region ;
        dcterms:name  "Highland"@en .
ex:speyside  a    place:Region ;
        dcterms:name  "Speyside"@en .
ex:ireland   a    place:Region ;
        dcterms:name  "Ireland"@en.

#Distillery

ex:laga      a    bevon:Distillery;
            dcterms:name        "Lagavulin"@en ; 
            npg:founded         1816 ;
            dcterms:hailsFrom   ex:islay .

ex:bowm      a    vcard:Region ;
            dcterms:name    "Bowmore"@en ; 
            npg:founded     1779 ;
            dcterms:hailsFrom   ex:highland ,
                                ex:speyside .

ex:glen      a    vcard:Region ;
            dcterms:name    "Glenmorangie"@en ; 
            npg:founded     1843 ;
            dcterms:hailsFrom   ex:speyside .

ex:redb      a    vcard:Region ;  
            dcterms:name    "Redbreast"@en ; 
            npg:founded     1903 ;
            dcterms:hailsFrom   ex:ireland .

